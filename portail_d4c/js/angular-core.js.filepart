                    if (attrs.d4cCollapsibleFixme) {
                        scope.$watch(attrs.d4cCollapsibleFixme, function (nv, ov) {
                            collapse = element.find('[d4c-collapse]');
                            panel = element.find('[d4c-collapse-panel]');
