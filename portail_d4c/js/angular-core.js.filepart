                                if (nv && !collapsible) {
                                    collapse.css('cursor', 'pointer');
                                    collapse.on('click', function toggle() {
                                        panel.toggle();
                                        collapsed = !collapsed;
                                    });
                                    collapsible = true;
                                    if (inList && autoCollapse) {
                                        panel.toggle();
                                        collapsed = true;
                                    }
                                } else if (!nv && collapsible) {
                                    collapse.css('cursor', 'inherit');
                                    collapse.off('click');
